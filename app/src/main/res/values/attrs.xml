<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BottomTab">
        <attr name="drawableTop" format="reference"/>
        <attr name="bottomTabText" format="string|reference"/>
        <attr name="bottomTabTextColor" format="color|reference"/>
        <attr name="bottomTabTextSize" format="dimension|reference"/>
        <attr name="hintText" format="string|reference"/>
        <attr name="hintTextColor" format="color|reference"/>
        <attr name="hintTextSize" format="dimension|reference"/>
        <attr name="hintTextBackground" format="reference"/>
        <attr name="hintEnable" format="boolean"/>
        <attr name="verticalSpace" format="dimension|reference"/>
    </declare-styleable>

    <declare-styleable name="ItemView">
        <attr name="ItemIcon" format="reference"/>
        <attr name="ItemText" format="string|reference"/>
    </declare-styleable>

    <declare-styleable name="ItemView2">
        <attr name="ItemKey" format="string|reference"/>
        <attr name="ItemValue" format="string|reference"/>
        <attr name="IsShowAvatar" format="boolean"/>
        <attr name="IsShowArrow" format="boolean"/>
        <attr name="IsShowCheck" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="ItemViewDivider">
        <attr name="LeftMargin" format="dimension|reference"/>
    </declare-styleable>
</resources>